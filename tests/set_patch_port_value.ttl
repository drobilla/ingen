@prefix ingen: <http://drobilla.net/ns/ingen#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .

<msg0>
	a patch:Put ;
	patch:subject <ingen:/root/in> ;
	patch:body [
		a lv2:InputPort ,
			lv2:ControlPort
	] .

<msg1>
	a patch:Set ;
	patch:subject <ingen:/root/in> ;
	patch:property ingen:value ;
	patch:value 0.5 .